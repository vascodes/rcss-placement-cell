<script>

            $(".contact-enquiry").submit(function (e) {
                e.preventDefault();
                var name = $('#name').val();
                var email = $('#email').val();
                var phone = $('#phone').val();
                var address = $('#address').val();
                var message = $('#message').val();
                grecaptcha.ready(function () {
                    grecaptcha.execute('6Lfz8BwbAAAAAJpb1soFrqbo8L-MloIUl8vlYjTz', {
                        action: 'contact'
                    }).then(function (token) {
                        var action = 'contact';
                        $.ajax({
                            type: "POST",
                            url: '/site/contact-enquiry',
                            data: {name: name, email: email, token: token, phone: phone, address: address, message: message, action: action},
                            success: function (data)
                            {
                                if (data == 1) {
                                    $('.contact-enquiry').prepend('<div id="email-alert" style="color: #28a745;font-weight: 600;margin-bottom:10px;font-size: 15px;">Your Contact Enquiry Send Successfully</div>');
                                    
                                    $('#name').val("");
                                    $('#email').val("");
                                    $('#phone').val("");
                                    $('#address').val("");
                                    $('#message').val("");
                                    $('#email-alert').delay(2000).fadeOut('slow');
                                }

                            }
                        });

                    });
                });
            });
</script>

